    "jest": "^26.6.3",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.1.1",
    "@tsconfig/node10": "^1.0.0",
    "typescript": "^4.1.3"
